package kr.co.fmos.customer;

public class InquiryDTO {

	private String inquiry_id;
	private String member_id;
	private int inquire_kind;
	private String inquiry_title;
	private String inquiry_content;
	private String inquiry_theater_name;
	private String inquiry_file;
	private int inquiry_pw;
	private String inquiry_answer;
	private String inquiry_datetime;
	
	public InquiryDTO() {}

	public String getInquiry_id() {
		return inquiry_id;
	}

	public void setInquiry_id(String inquiry_id) {
		this.inquiry_id = inquiry_id;
	}

	public String getMember_id() {
		return member_id;
	}

	public void setMember_id(String member_id) {
		this.member_id = member_id;
	}

	public int getInquire_kind() {
		return inquire_kind;
	}

	public void setInquire_kind(int inquire_kind) {
		this.inquire_kind = inquire_kind;
	}

	public String getInquiry_title() {
		return inquiry_title;
	}

	public void setInquiry_title(String inquiry_title) {
		this.inquiry_title = inquiry_title;
	}

	public String getInquiry_content() {
		return inquiry_content;
	}

	public void setInquiry_content(String inquiry_content) {
		this.inquiry_content = inquiry_content;
	}

	public String getInquiry_theater_name() {
		return inquiry_theater_name;
	}

	public void setInquiry_theater_name(String inquiry_theater_name) {
		this.inquiry_theater_name = inquiry_theater_name;
	}

	public String getInquiry_file() {
		return inquiry_file;
	}

	public void setInquiry_file(String inquiry_file) {
		this.inquiry_file = inquiry_file;
	}

	public int getInquiry_pw() {
		return inquiry_pw;
	}

	public void setInquiry_pw(int inquiry_pw) {
		this.inquiry_pw = inquiry_pw;
	}

	public String getInquiry_answer() {
		return inquiry_answer;
	}

	public void setInquiry_answer(String inquiry_answer) {
		this.inquiry_answer = inquiry_answer;
	}

	public String getInquiry_datetime() {
		return inquiry_datetime;
	}

	public void setInquiry_datetime(String inquiry_datetime) {
		this.inquiry_datetime = inquiry_datetime;
	}

	@Override
	public String toString() {
		return "InquireDTO [inquiry_id=" + inquiry_id + ", member_id=" + member_id + ", inquire_kind=" + inquire_kind
				+ ", inquiry_title=" + inquiry_title + ", inquiry_content=" + inquiry_content
				+ ", inquiry_theater_name=" + inquiry_theater_name + ", inquiry_file=" + inquiry_file + ", inquiry_pw="
				+ inquiry_pw + ", inquiry_answer=" + inquiry_answer + ", inquiry_datetime=" + inquiry_datetime + "]";
	}
	
	
}
